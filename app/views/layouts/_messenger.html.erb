<div class="messenger-tab centered noselect">
  <div>Need a dev team? Hire us</div>
  <div class="images">
    <img class="circle" src="/images/andrew.jpeg" />
    <img class="circle" src="/images/doug.jpg" />
    <img class="circle" src="/images/majo.jpg" />
  </div>
</div>

<script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '95100348886',
      xfbml      : true,
      version    : 'v2.6'
    });
  };

  (function(d, s, id){
    var js, fjs = d.getElementsByTagName(s)[0];
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/en_US/sdk.js";
    fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));

  $('.messenger-tab').on('click', function() {
    $('.messenger-popup').toggleClass('active');
  })
</script>

<div class="messenger-popup">
  <div class="fb-page"
       data-href="https://www.facebook.com/simplecto/"
       data-tabs="messages"
       data-width="400"
       data-height="300"
       data-small-header="true"
       data-show-facepile="true"
       data-adapt-container-width="true">
    <div class="fb-xfbml-parse-ignore">
      <blockquote></blockquote>
    </div>
  </div>
</div>
