<div id="question-index">
<%= render partial: "layouts/navigation_main" %>

<div class="container-fluid">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <h1 class="sub-nav">Most Popular Answers</h1>
      <%= link_to "My Questions", user_questions_path(current_user), class: "sub-nav" %>
      <% @questions.each do |question| %>
        <%= render "preview", question: question %>
      <% end %>
    </div>
  </div>
</div>
